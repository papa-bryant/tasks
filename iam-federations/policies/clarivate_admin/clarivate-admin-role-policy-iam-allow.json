{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "adminAllowIamIpRestrictedClarivate",
      "Effect": "Allow",
      "Action": [
        "iam:UntagRole",
        "iam:UpdateUser",
        "iam:UpdateLoginProfile",
        "iam:UpdateGroup",
        "iam:UpdateAssumeRolePolicy",
        "iam:UpdateAccessKey",
        "iam:UpdateRoleDescription",
        "iam:TagRole",
        "iam:SimulatePrincipalPolicy",
        "iam:SimulateCustomPolicy",
        "iam:RemoveUserFromGroup",
        "iam:RemoveRoleFromInstanceProfile",
        "iam:List*",
        "iam:Get*",
        "iam:GenerateServiceLastAccessedDetails",
        "iam:GenerateCredentialReport",
        "iam:DetachUserPolicy",
        "iam:DetachRolePolicy",
        "iam:DetachGroupPolicy",
        "iam:DeleteUserPolicy",
        "iam:DeleteUser",
        "iam:DeleteRole",
        "iam:DeletePolicyVersion",
        "iam:DeletePolicy",
        "iam:DeleteLoginProfile",
        "iam:DeleteInstanceProfile",
        "iam:DeleteGroupPolicy",
        "iam:DeleteGroup",
        "iam:DeleteAccessKey",
        "iam:CreateUser",
        "iam:CreateServiceLinkedRole",
        "iam:CreateRole",
        "iam:CreatePolicyVersion",
        "iam:CreatePolicy",
        "iam:CreateLoginProfile",
        "iam:CreateInstanceProfile",
        "iam:CreateGroup",
        "iam:CreateAccessKey",
        "iam:ChangePassword",
        "iam:AttachUserPolicy",
        "iam:AttachRolePolicy",
        "iam:AttachGroupPolicy",
        "iam:AddUserToGroup",
        "iam:CreateServiceSpecificCredential",
        "iam:ResetServiceSpecificCredential",
        "iam:DeleteServiceSpecificCredential",
        "iam:UntagSAMLProvider",
        "iam:UntagRole",
        "iam:TagRole",
        "iam:TagMFADevice",
        "iam:TagSAMLProvider",
        "iam:UntagServerCertificate",
        "iam:TagPolicy",
        "iam:TagUser",
        "iam:UntagUser",
        "iam:UntagMFADevice",
        "iam:TagServerCertificate",
        "iam:UntagPolicy",
        "iam:UntagOpenIDConnectProvider",
        "iam:UntagInstanceProfile",
        "iam:TagOpenIDConnectProvider",
        "iam:TagInstanceProfile",
        "iam:UpdateRoleDescription"
      ],
      "Resource": [
        "arn:aws:iam::${account_id}:user/cl/custom/*",
        "arn:aws:iam::${account_id}:user/cl/app/*",
        "arn:aws:iam::${account_id}:user/cl/admin/*",
        "arn:aws:iam::${account_id}:role/cl/custom/*",
        "arn:aws:iam::${account_id}:role/cl/app/*",
        "arn:aws:iam::${account_id}:role/cl/admin/*",
        "arn:aws:iam::${account_id}:policy/cl/custom/*",
        "arn:aws:iam::${account_id}:policy/cl/app/*",
        "arn:aws:iam::${account_id}:policy/cl/admin/*",
        "arn:aws:iam::${account_id}:instance-profile/cl/custom/*",
        "arn:aws:iam::${account_id}:instance-profile/cl/app/*",
        "arn:aws:iam::${account_id}:instance-profile/cl/admin/*",
        "arn:aws:iam::${account_id}:group/cl/custom/*",
        "arn:aws:iam::${account_id}:group/cl/app/*",
        "arn:aws:iam::${account_id}:group/cl/admin/*"
      ]
    }
  ]
}
